{
  "name": "api-node-side-quest",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "pre-commit": "lint-staged",
    "test": "jest --silent --colors --noStackTrace --passWithNoTests",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage"
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "npm run test:staged"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "husky": "^5.0.4",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "standard": "^16.0.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.3",
    "validator": "^13.5.1"
  }
}
